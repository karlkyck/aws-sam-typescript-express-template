{
  "name": "aws-sam-typescript-template",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf -v dist/*",
    "lint": "tslint --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "audit": "npm audit && snyk test",
    "compile": "tsc",
    "test": "jest --ci",
    "copydependencies": "copy-node-modules . dist",
    "build": "run-s clean lint audit compile test copydependencies",
    "deploy": "node scripts/deploy.js -n $npm_package_name"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.11",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "aws-sdk": "^2.814.0",
    "copy-node-modules": "^1.0.7",
    "jest": "^23.5.0",
    "jest-xml-matcher": "^1.1.1",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.3",
    "snyk": "^1.95.1",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "yargs": "^12.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.+\\.test\\.ts)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
